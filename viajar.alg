algoritmo "viajar"

var
  valor,A:inteiro
inicio
    Escreva("Para você viajar no rato, você precisa de 1000$.")
    Escreva("Quanto dinheiro você tem? ")
    Leia(valor)
    A<-1000
    Se (valor>=A) entao
       Escreva("Partiu viajar cuzao.")
    Senao
         Escreva("Que bad my bro.")
    fimse
fimalgoritmo


